<?php
class AddressParsingTest extends DrupalWebTestCase {

  public static function getInfo() {
    return array(
      'name' => t('Address Parsing using Google Maps Api'), 
      'desc' => t('Parses a address (string) into an array with the different parts of the address'), 
      'group' => t('MyCommunity.org'),
    );
  }

  function setUp() {
    parent::setUp('mt_event_feed');
  }

  function testAddressParsingOne() {
    $address = "New Work City (412 Broadway, Floor 2, New York, NY 10013)";
    $address_parts = mt_event_feed_getAddressParts($address);
    $this->assertEqual($address_parts['name'] , 'New Work City', "The name of the location matches");
    $this->assertEqual($address_parts['address'] , '412 Broadway', "The address matches");
    $this->assertEqual($address_parts['city'] , 'New York', "The name of the city matches");
    $this->assertEqual($address_parts['state'] , 'NY', "The name of the state matches");
    $this->assertEqual($address_parts['zip'] , '10013', "The zip matches");
  }

