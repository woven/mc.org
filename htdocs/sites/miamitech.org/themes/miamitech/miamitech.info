name = "MiamiTech.org Theme" 
description = "Theme for MiamiTech.org"
core = "6.x"
engine = "phptemplate"
base theme = "tao"
 
; Regions
regions[header] = Header
regions[branding] = Branding
regions[featured] = Featured
regions[group_logo] = Group Logo
regions[footer] = Footer
regions[content] = Content
regions[left] = Left
regions[right] = Right
regions[below_content] = Below Content
regions[above_content] = Above Content
regions[content_top] = Content Top
regions[content_bottom] = Content Bottom
regions[footer_above] = Footer Above - Sitemap
regions[overlay] = Overlay

; Scripts
scripts[] = "js/jcarousel/lib/jquery.jcarousel.js"
scripts[] = "js/main.js"
scripts[] = "js/jquery.tools.min.js"
scripts[] = "js/custom.js"
scripts[] = "js/jquery.elastic.source.js"
scripts[] = "js/elastic.js"

; Stylesheets
stylesheets[screen][] = "css/reset.css"
stylesheets[screen][] = "css/drupal.css"
stylesheets[screen][] = "css/base.css"
stylesheets[screen][] = "css/main.css"
stylesheets[screen][] = "css/custom.css"
stylesheets[screen][] = "css/custom-less.css"
stylesheets[screen][] = "css/mt.css"
stylesheets[screen][] = "js/colorbox/colorbox.css"
stylesheets[all][] = "views_gallery.css"
stylesheets[screen][] = "css/buttons.css"

; conditional styles for ie 7 and 6
conditional-stylesheets[if lte IE 7][all][] = css/ie.css
conditional-stylesheets[if lte IE 6][all][] = css/ie6.css