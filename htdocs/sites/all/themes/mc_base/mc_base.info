name = "MyCommunity.org Base Theme" 
description = "Base Theme for MyCommunity.org"
core = "6.x"
engine = "phptemplate"
 
; Regions
regions[header] = Header
regions[branding] = Branding
regions[featured] = Featured
regions[group_logo] = Group Logo
regions[footer] = Footer
regions[content] = Content
regions[left] = Left
regions[right] = Right
regions[below_content] = Below Content
regions[above_content] = Above Content
regions[content_top] = Content Top
regions[content_bottom] = Content Bottom
regions[footer_above] = Footer Above - Sitemap
regions[overlay] = Overlay

; Scripts
scripts[] = "js/jcarousel/lib/jquery.jcarousel.js"
scripts[] = "js/jquery.livequery.js"
scripts[] = "js/jquery.tools.min.js"
scripts[] = "jquery.qtip.min.js"
scripts[] = "js/main.js"
scripts[] = "js/custom.js"
scripts[] = "js/jquery.elastic.source.js"
scripts[] = "js/elastic.js"
scripts[] = "js/qtip.js"

;========= START: Stylesheets & CSS =========
;disable core styles
stylesheets[all][] = "user.css"
stylesheets[all][] = "node.css"
stylesheets[all][] = "ctools.css"
stylesheets[all][] = "defaults.css"
stylesheets[all][] = "system-menus.css"
stylesheets[all][] = "system.css"
stylesheets[all][] = "filefield.css"
stylesheets[all][] = "modalframe/css/modalframe.parent.css"
stylesheets[all][] = "modalframe/css/modalframe.child.css"
stylesheets[all][] = "event/event.css"
stylesheets[all][] = "views_gallery.css"

; Styles: from http://drupal.org/project/tao
stylesheets[all][] = "css/reset.css"
stylesheets[all][] = "css/drupal.css"
stylesheets[all][] = "css/base.css"

; Styles: theme/styles stylesheets
stylesheets[all][] = "css/tabs.css"
stylesheets[all][] = "css/all.css"

; Styles known components
stylesheets[all][] = "css/search.css"
stylesheets[all][] = "css/buttons.css"

; Styles: stand alone css component
stylesheets[all][] = "css/jquery.qtip.min.css"
